{
    "RoslynExtensionsOptions": {
        "enableAnalyzersSupport": true
    },
    "FormattingOptions": {
        "enableEditorConfigSupport": true
    },
    "msbuild": {
        "enabled": true,
        "loadProjectsOnDemand": true,
        "GenerateBinaryLogs": false
    },
    "fileOptions": {
        "systemExcludeSearchPatterns": [
            "**/node_modules/**/*",
            "**/bin/**/*",
            "**/obj/**/*",
            "**/node_modules/**/*"
        ],
        "excludeSearchPatterns": []
    }
}
